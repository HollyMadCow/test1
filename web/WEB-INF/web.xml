<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <resource-ref>
        <description>yhwj</description>
        <res-ref-name>jdbc/ConnectionPool</res-ref-name>
        <res-type>javax.sql.DataSource</res-type>
        <res-auth>Container</res-auth>
    </resource-ref>
    <servlet>
        <servlet-name>pkilogin</servlet-name>
        <servlet-class>cn.yhgaj.wjdd.pkilogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>pkilogin</servlet-name>
        <url-pattern>/servlet/ndp/pkilogin</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>search</servlet-name>
        <servlet-class>cn.yhgaj.wjdd.search</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>search</servlet-name>
        <url-pattern>/servlet/ndp/search</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>addcase</servlet-name>
        <servlet-class>cn.yhgaj.wjdd.addcase</servlet-class>
        <!--<load-on-startup>1</load-on-startup>-->
    </servlet>
    <servlet-mapping>
        <servlet-name>addcase</servlet-name>
        <url-pattern>/servlet/ndp/addcase</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>uploadimg</servlet-name>
        <servlet-class>cn.yhgaj.wjdd.uploadimg</servlet-class>
        <!--<load-on-startup>1</load-on-startup>-->
    </servlet>
    <servlet-mapping>
        <servlet-name>uploadimg</servlet-name>
        <url-pattern>/servlet/uploadimg</url-pattern>
    </servlet-mapping>
</web-app>